#%RAML 1.0
title: Amazon S3 REST API
version: 1
mediaType: application/json

types:
  person:
    type: object
    properties:
      name: string
      age?: number

  manager:
    type: person

/users/:
  get:
    responses:
      200:
        body:
          application/json:
            type: manager
            example:
              {
                name: "Martin",
                age: 45
              }
