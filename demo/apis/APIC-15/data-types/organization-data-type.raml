#%RAML 1.0 DataType
type: object
properties: 
  alternate_id?: 
    type: number | nil
    description: The alternate ID of the organization
  category:
    description: Contains the ID and name of the category the organization is in
    type: object
    properties: 
      id: 
        type: number
        description: ID of the category
      name: 
        type: string
        description: The name of the category
  created_at:
    type: datetime
    description: The date of creation of the organization
